{
  "App": {
    
  },
  "IpGeolocationSettings": {
    "BaseAddress": "https://ipapi.co/",
    "CacheEnabled": false,
    "CacheExpiration": 2,
    "CacheExpirationType": "Hours",
    "CacheType": "MemoryAndFile"
  },
  "Serilog" : {
    "MinimalLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 20
        }
      }
    ]
  }
}